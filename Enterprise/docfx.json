{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {   
      "layout": "Conceptual",      
      "breadcrumb_path": "/office365/enterprise/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "bilingual_type": "hover over",
      "searchScope": ["Office 365 Enterprise"]
    },
      
    "fileMetadata": {},
    "template": [],
    "dest": "OfficeDocs-Enterprise"
  }
}
